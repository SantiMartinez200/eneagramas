# ========================================
# Laravel 12 - Archivo .gitignore
# ========================================

# Directorios de dependencias
/vendor/
/node_modules/

# Archivos de entorno
.env
.env.backup
.env.local
.env.production
.env.development

# Archivos de caché y compilados
bootstrap/cache/*.php
!bootstrap/cache/.gitignore

# Compilaciones de Vite y Mix
/public/hot
/public/storage
/storage/*.key
/public/build
/public/js/*.js
/public/css/*.css

# Archivos de depuración y logs
/storage/logs/*.log
/storage/debugbar/*
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*
/storage/app/public/*
/storage/app/tmp/*
/storage/app/*.tmp
/storage/app/*.bak
/storage/app/*.log

# Ignorar backups o dumps
*.sql
*.sqlite
*.bak
*.backup
*.tmp
*.old

# IDEs y editores
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Sistema operativo
.DS_Store
Thumbs.db
ehthumbs.db
desktop.ini

# Archivos de npm/yarn/pnpm
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
package-lock.json
yarn.lock
pnpm-lock.yaml

# Tests y coverage
.phpunit.result.cache
coverage/
.phpunit.cache

# Otros
*.swp
*.swo
*.orig
*.log
*.pid
*.seed
*.zip
*.tar
*.gz

# ========================================
# Fin del archivo
# ========================================
